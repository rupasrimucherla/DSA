import java.util.Scanner;
public class Main
{
    public static int largest(int[] arr) {
        int max = arr[0];
        for (int i = 1; i < arr.length; i++) {
            if (arr[i] > max) {
                max = arr[i];
            }
        }
        return max;
    }
    public static void main(String[] args) {
        int[] nums={2,14,6,7,84,24};
        int Maxnum=largest(nums);
        System.out.println(Maxnum);
	}
}
